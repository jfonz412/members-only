<% provide(:title, "New Post") %>
<h1>New Post</h1>
<%= form_for :post, url: posts_path do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title, class: 'form-control' %>

	<%= f.label :body %>
	<%= f.text_area :body, class: 'form-control' %>

	<%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>